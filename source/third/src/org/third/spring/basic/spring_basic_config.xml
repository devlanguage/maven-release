<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
  xmlns:tx="http://www.springframework.org/schema/tx"
  xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
      http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
      http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd"
>
  <import resource="../spring_common_config.xml" />
  <bean id="user" name="u1, u2 u3;u4" class="org.third.testdata.user.domain.User"></bean>

  <bean id="bf_PostProcessor" class="org.third.spring.basic.postprocessor.BeanFactoryPostProcessor_impl"></bean>
  <bean id="bean_PostProcessor" class="org.third.spring.basic.postprocessor.BeanPostProcessor_impl"></bean>

  <bean id="aware_BeanClassLoader" class="org.third.spring.basic.aware.BeanClassLoaderAware_impl"></bean>
  <bean id="aware_BeanName" class="org.third.spring.basic.aware.BeanFactoryAware_impl"></bean>
  <bean id="aware_BeanFactory" class="org.third.spring.basic.aware.BeanNameAware_impl"></bean>
</beans>