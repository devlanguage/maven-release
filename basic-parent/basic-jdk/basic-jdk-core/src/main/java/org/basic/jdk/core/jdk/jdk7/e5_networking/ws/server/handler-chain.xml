<handler-chains xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee">
 <handler-chain>
  <handler>
   <handler-name>authHandler</handler-name>
   <handler-class>org.basic.jdk6.ws.server.handlers.HelloAuthHandlerServer</handler-class>
  </handler>
  <handler>
   <handler-name>optionalHandler</handler-name>
   <handler-class>org.basic.jdk6.ws.server.handlers.HelloOptionalHandlerServer</handler-class>
  </handler>
 </handler-chain>
</handler-chains>  
 